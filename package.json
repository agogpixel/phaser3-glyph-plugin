{
  "name": "@agogpixel/phaser3-glyph-plugin",
  "version": "0.0.0",
  "description": "Phaser 3 glyph plugin.",
  "main": "main.bundle.js",
  "module": "main.bundle.js",
  "scripts": {
    "build": "cross-env NODE_ENV=production webpack --progress --color",
    "build:canvas": "cross-env NODE_ENV=production WEBGL_RENDERER=false webpack --progress --color",
    "build:webgl": "cross-env NODE_ENV=production CANVAS_RENDERER=false webpack --progress --color",
    "start": "jest --watch",
    "test": "jest --coverage",
    "lint": "eslint jest.config.js webpack.config.js src test && prettier --check .",
    "lint:fix": "eslint --fix jest.config.js webpack.config.js src test && prettier --write .",
    "prepare": "husky install"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/agogpixel/phaser3-glyph-plugin.git"
  },
  "author": "kidthales <kidthales@agogpixel.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/agogpixel/phaser3-glyph-plugin/issues"
  },
  "homepage": "https://github.com/agogpixel/phaser3-glyph-plugin#readme",
  "dependencies": {
    "@agogpixel/phaser3-ts-utils": "0.2.0-alpha--dev-build-1639454098",
    "color-string": "^1.9.0"
  },
  "peerDependencies": {
    "phaser": ">=3.60.0-beta.3"
  },
  "devDependencies": {
    "@types/color-string": "^1.5.2",
    "@types/jest": "^27.0.3",
    "@types/node": "^14.17.27",
    "@typescript-eslint/eslint-plugin": "^5.8.1",
    "@typescript-eslint/parser": "^5.8.1",
    "copy-webpack-plugin": "^10.2.0",
    "css-loader": "^6.5.1",
    "cross-env": "^7.0.3",
    "eslint": "^8.5.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "html-loader": "^3.0.1",
    "husky": "^7.0.4",
    "jest": "^27.4.5",
    "jest-canvas-mock": "^2.3.1",
    "lint-staged": "^12.1.4",
    "mini-css-extract-plugin": "^2.4.5",
    "phaser": "^3.60.0-beta.3",
    "prettier": "^2.5.1",
    "sass": "^1.45.1",
    "sass-loader": "^12.4.0",
    "shx": "^0.3.3",
    "source-map-loader": "^3.0.0",
    "style-loader": "^3.3.1",
    "ts-jest": "^27.1.2",
    "ts-loader": "^9.2.6",
    "tslib": "^2.3.1",
    "typescript": "^4.5.4",
    "webpack": "^5.65.0",
    "webpack-cli": "^4.9.1"
  },
  "engines": {
    "node": ">=14.0.0",
    "npm": ">=6.0.0"
  },
  "lint-staged": {
    "*.(js|ts)": "eslint --cache --fix",
    "*.(html|css|less|scss|sass|styl|md|json|yml)": "prettier --write"
  }
}

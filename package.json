{
  "name": "@agogpixel/phaser3-glyph-plugin",
  "version": "0.1.0",
  "description": "Phaser 3 glyph plugin.",
  "main": "main.bundle.js",
  "module": "main.bundle.js",
  "types": "index.d.ts",
  "scripts": {
    "postinstall": "husky install",
    "start": "jest --watch",
    "start-demos": "cross-env DEMOS=true webpack-dev-server --progress --color",
    "test": "jest --coverage",
    "prebuild": "cavy clean dist",
    "build": "npm run build-bundle && npm run build-canvas-bundle && npm run build-webgl-bundle && npm run build-demos && npm run build-js && npm run build-docs",
    "postbuild": "cavy copy dist && shx cp LICENSE README.md dist",
    "build-bundle": "cross-env NODE_ENV=production webpack --progress --color",
    "build-canvas-bundle": "cross-env NODE_ENV=production WEBGL_RENDERER=false webpack --progress --color",
    "build-webgl-bundle": "cross-env NODE_ENV=production CANVAS_RENDERER=false webpack --progress --color",
    "build-demos": "cross-env NODE_ENV=production DEMOS=true webpack --progress --color",
    "build-js": "tsc --project tsconfig.build.json",
    "build-docs": "typedoc --tsconfig tsconfig.build.json --out dist/docs --entryPointStrategy expand src && shx cp LICENSE dist/docs",
    "create-tarball": "cavy pack dist",
    "publish-tarball": "cavy publish dist",
    "lint": "cavy lint --all",
    "lint-fix": "cavy lint --all --fix",
    "clean": "cavy clean --all"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/agogpixel/phaser3-glyph-plugin.git"
  },
  "author": "kidthales <kidthales@agogpixel.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/agogpixel/phaser3-glyph-plugin/issues"
  },
  "homepage": "https://github.com/agogpixel/phaser3-glyph-plugin#readme",
  "dependencies": {
    "@agogpixel/phaser3-ts-utils": "0.2.0-alpha--dev-build-1639454098",
    "color-string": "^1.9.0"
  },
  "peerDependencies": {
    "phaser": ">=3.60.0-beta.3"
  },
  "devDependencies": {
    "@agogpixel/cavy": "^0.1.0",
    "@types/color-string": "^1.5.2",
    "@types/jest": "^27.0.3",
    "@types/node": "^14.17.27",
    "@typescript-eslint/eslint-plugin": "^5.8.1",
    "@typescript-eslint/parser": "^5.8.1",
    "copy-webpack-plugin": "^10.2.0",
    "cross-env": "^7.0.3",
    "css-loader": "^6.5.1",
    "ejs-loader": "^0.5.0",
    "eslint": "^8.5.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "html-loader": "^3.0.1",
    "html-webpack-plugin": "^5.5.0",
    "husky": "^7.0.4",
    "jest": "^27.4.5",
    "jest-webgl-canvas-mock": "^0.2.3",
    "lint-staged": "^12.1.4",
    "mini-css-extract-plugin": "^2.4.5",
    "phaser": "^3.60.0-beta.3",
    "prettier": "^2.5.1",
    "sass": "^1.45.1",
    "sass-loader": "^12.4.0",
    "shx": "^0.3.3",
    "source-map-loader": "^3.0.0",
    "style-loader": "^3.3.1",
    "ts-jest": "^27.1.2",
    "ts-loader": "^9.2.6",
    "tslib": "^2.3.1",
    "typedoc": "^0.22.10",
    "typedoc-plugin-rename-defaults": "^0.4.0",
    "typescript": "^4.5.4",
    "webpack": "^5.65.0",
    "webpack-cli": "^4.9.1",
    "webpack-dev-server": "^4.7.2"
  },
  "engines": {
    "node": ">=14.0.0",
    "npm": ">=6.0.0"
  },
  "lint-staged": {
    "*.(js|ts)": "eslint --cache --fix",
    "*.(html|css|less|scss|sass|styl|md|json|yml)": "prettier --write"
  }
}
